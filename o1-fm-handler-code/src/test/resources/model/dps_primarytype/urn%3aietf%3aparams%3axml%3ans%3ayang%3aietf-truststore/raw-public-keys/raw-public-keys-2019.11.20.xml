    <ns1:type xsi:type="ns1:stringType">
      <ns1:notNullConstraint/>
    </ns1:type>
  </ns2:primaryTypeAttribute>
  <ns2:primaryTypeAttribute immutable="false" key="false" mandatory="false" ns="urn:ietf:params:xml:ns:yang:ietf-truststore" ns1:lifeCycle="CURRENT" ns1:name="description" readBehavior="FROM_PERSISTENCE" sensitive="false" writeBehavior="PERSIST_AND_DELEGATE">
