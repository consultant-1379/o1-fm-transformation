--><ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_common" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" hierarchyType="HIERARCHICAL" ns1:definedBy="NE" ns1:lifeCycle="CURRENT" ns1:name="raw-public-keys" ns1:ns="urn:ietf:params:xml:ns:yang:ietf-truststore" ns1:version="2019.11.20" readBehavior="FROM_PERSISTENCE" writeBehavior="PERSIST_AND_DELEGATE">
